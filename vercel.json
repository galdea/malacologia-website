{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app/page.tsx"
      }
    ],
    "functions": {
      "api/**/*.ts": {
        "maxDuration": 10
      }
    }  }
  